{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "autoApprove": [
        "fetch"
      ]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time"
      ]
    },
    "weather": {
      "command": "npx",
      "args": [
        "-y",
        "@StevenStavrakis/mcp-starter-template"
      ]
    }
  }
}